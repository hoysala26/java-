import java.util.*;

public class Strings {
    public static void main(String args[]) {
      String a = "eabcdef";
      String result=" ";


      for(int i=0; i<a.length(); i++) {
        if(a.charAt(i) == 'e') {
          result += 'i';
        } else {
          result += a.charAt(i);
        }
      }


      System.out.println(result);
    }
}
