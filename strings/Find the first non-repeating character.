public class hello {
    public static void main(String[] args) {
        String name = "programming";
        String result = "";

        for(int i=0;i<name.length();i++){
            char ch=name.charAt(i);

            boolean found = false;
            for(int j=0;j<name.length();j++){
                if(i!=j&&name.charAt(j)==ch){
                    found=true;
                    break;
                }
            }
            if(!found){
                System.out.println(ch);
                break;
                
            }
            
        }
        
       
    }
}
