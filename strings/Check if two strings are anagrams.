import java.util.Arrays;

public class Hello {
    public static void main(String[] args) {
        String first = "listen";
        String last = "silent";

        // Step 1: Convert to char array
        char[] arr1 = first.toCharArray();
        char[] arr2 = last.toCharArray();

        // Step 2: Sort arrays
        Arrays.sort(arr1);
        Arrays.sort(arr2);

        // Step 3: Compare
        if(Arrays.equals(arr1, arr2)){
            System.out.println("Yes, Anagrams");
        } else {
            System.out.println("No, Not Anagrams");
        }
    }
}
